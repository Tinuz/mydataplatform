{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "https://data-platform.example.com/schemas/dim_bank_account.json",
  "title": "Bank Account Dimension",
  "description": "dbt dimension table for bank accounts",
  "type": "object",
  "required": ["account_key", "iban"],
  "properties": {
    "account_key": {
      "type": "string",
      "description": "Surrogate key for bank account (generated from IBAN)"
    },
    "iban": {
      "type": "string",
      "pattern": "^[A-Z]{2}[0-9]{2}[A-Z0-9]+$",
      "description": "International Bank Account Number"
    }
  },
  "additionalProperties": false
}
